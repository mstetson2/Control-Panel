package com.nolimitsframework.easing;

public class EaseInOutQuart extends Easing {

    public double getValue(double t, double b, double c, double d) {
        if ((t /= d / 2) < 1) {
            return c / 2 * t * t * t * t + b;
        }
        return -c / 2 * ((t -= 2) * t * t * t - 2) + b;
    }
}
